{
    "$schema": "https://typedoc.org/schema.json",
    "intentionallyNotExported": [
        "ComponentClass",
        "AbstractComponent",
        "ConverterComponent",
        "RendererComponent",
        "SORT_STRATEGIES",
        "_ModelToObject",
        "EventHooksMomento",
        "MarkedPlugin",
        "MeaningKeywords"
    ],
    "sort": ["kind", "instance-first", "required-first", "alphabetical"],
    "entryPoints": ["../src/index.ts"],
    "excludeExternals": true,
    "excludeInternal": false,
    "excludePrivate": true,
    "treatWarningsAsErrors": false,
    "validation": {
        "notExported": true,
        "invalidLink": true,
        "notDocumented": false
    },
    "visibilityFilters": {
        "protected": false,
        "private": false,
        "inherited": true,
        "external": false,
        "@internal": false
    },
    "searchGroupBoosts": {
        "Classes": 2.0,
        "Interfaces": 2.0,
        "Enumerations": 2.0,
        "Type Aliases": 2.0
    },
    "searchInComments": true,
    "logLevel": "Verbose"
}
