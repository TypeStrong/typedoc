{{#compact}}
    {{{wbr name}}}<span class="tsd-signature-symbol">(</span>
    {{#each parameters}}
        {{#if @index}},&nbsp;{{/if}}
        {{name}}
        <span class="tsd-signature-symbol">
            {{#if isOptional}}?{{/if}}
            :&nbsp;
        </span>
        {{#with type}}{{>type}}{{/with}}
        {{#if defaultValue}}
            <span class="tsd-signature-symbol">&nbsp;=&nbsp;</span>
            {{defaultValue}}
        {{/if}}
    {{/each}}
    <span class="tsd-signature-symbol">)</span>
    {{#if type}}
        <span class="tsd-signature-symbol">:&nbsp;</span>
        {{#with type}}{{>type}}{{/with}}
    {{/if}}
{{/compact}}